<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.qdunavigation.mapper.EdgeMapper">
    <select id = "findAll" resultType="org.example.qdunavigation.pojo.Edge">
        select * from navi.edge
    </select>
    <select id="findById" resultType="org.example.qdunavigation.pojo.Edge">
        select * from navi.edge where edgeno = #{id}
    </select>
    <select id="findByNode" resultType="org.example.qdunavigation.pojo.Edge">
        select * from navi.edge where srcNode = #{nid} or tarNode = #{nid}
    </select>
</mapper>